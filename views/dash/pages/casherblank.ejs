<script>
    $(document).ready(function() {
        $('#dataTables-example').DataTable({
            responsive: true
        })
        var t = setTimeout(time,1000);//开始执行
        function time()
        {
            clearTimeout(t);//清除定时器
            dt = new Date();
            var h=dt.getHours();
            var m=dt.getMinutes();
            var s=dt.getSeconds();
            $("small").text("current time："+((Number(h)>=10)?h:("0"+h))+":"+((Number(m)>=10)?m:("0"+m))+":"+((Number(s)>=10)?s:("0"+s)))
            t = setTimeout(time,1000); //设定定时器，循环执行
        }
        $("#click").click(function(){
            $.post("/casher",
                    {
                        data1:$("#data1").val()
                },
                function(data){
                    var a = $(data).find("m2").length
                    var b = $(data).find("m2").text()
                    if(a==0){
                        $("#data1").val("")
                        $("#data2").val("")
                        return
                    }
                    var con = "<tr><th>"+($("#table38").children().length+1)+"</th><th>"+$("#data1").val()+"</th><th>"+$("#data2").val()+"</th><th>"+b+"</th><th>"+Number(b)*Number($("#data2").val())+"</th></tr>"
                    $("#table38").append(con)
                    $("#h1").text(Number($("#h1").text())+Number(b)*Number($("#data2").val()))
                    $("#data1").val("")
                    $("#data2").val("")
                }
            )

        })
        $("#pay").click(function(){
            var ss = '<div>sum:<input value="'+$("#h1").text()+'"></div>'
                    +'<div style="height: 30px"></div>'
                      +'<div>delivery:<input id="ggg"></div>'
                    +'<div style="height: 30px"></div>'
                        +'<div>change:<input id="ggg1"></div>'
            $.MsgBox.Confirm("PAY", ss);
        })
    });
</script>

